generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model usuario {
  id            String  @id @default(uuid()) @map("_id")
  email         String  @unique
  name          String
  doc           String  @unique
  phone_number  String
  tentativas    Int     @default(0)
  password      String
  active_acount Boolean @default(false)
  roles         String  @default("COMMON_USER")
}
